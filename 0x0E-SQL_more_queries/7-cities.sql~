-- script that creates the database hbtn_0d_usa and the table cities

-- CREATE DATABASE hbtn_0d_usa
CREATE IF NOT EXISTS hbtn_0d_usa;
USE hbtn_0d_usa;
-- CREATE TABLE cities
CREATE TABLE IF NOT EXISTS cities (id INT UNIQUE AUTO_INCREMENT PRIMARY KEY NOT NULL,
state_id INT FOREIGN KEY NOT NULL REFERENCES states (id), name VARCHAR(256) NOT NULL);
